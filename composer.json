{
    "name": "ray/ray-di-for-laravel",
    "description": "Ray.Di for Laravel",
    "keywords": ["di", "aop", "laravel"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ray\\RayDiForLaravel\\": "src/"
        },
        "files": ["src-files/apcu.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Ray\\RayDiForLaravel\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ray/di": "^2.15.1",
        "ray/compiler": "^1.10",
        "doctrine/cache": "^1.10 || ^2.2"
    },
    "require-dev": {
        "laravel/framework": "^8.83 || ^9.8 || ^10.0 || ^11.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "suggest": {
      "ext-apcu": "Needed to use ApcuCacheProvider"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
